<div class="content-wrapper-form"> 
<div class="content view-profile" style="margin-top:10%; margin-bottom:20%">
        <p class="profile-form-header">Hey <%= user.name + '!!!' %></p>
        <form class="pure-form pure-form-single">
            <fieldset>

            <div class='profile-display'>
            	<br></br>

            	<li><h>User Name: </h><% 
               	if (user.name === null) { 
            		%><% 
        		} else {
           			%><h style="float:right"><%=user.name%></h><%
        		} %></li>

        		<li><h>Contact: </h><% 
               	if (user.contact === null) { 
            		%><% 
        		} else {
           			%><h style="float:right"><%=user.contact%></h><%
        		} %></li>

        		<li><h>Date of Birth: </h><% 
               	if (user.DOB === null) { 
            		%><% 
        		} else {
           			var t = user.DOB.split(/[- :]/);
                var k = t[2].split(/[T]/);
                var d = t[0] + "-" + t[1] + "-" + k[0];
                %><h style="float:right"> <%=d%></h> <%
        		} %></li>
        	</div><br></br>

        	<button type="submit" id="edit-profile" class="pure-button pure-button-primary">Edit Profile</button>
                
            </fieldset>
        </form>

    </div>